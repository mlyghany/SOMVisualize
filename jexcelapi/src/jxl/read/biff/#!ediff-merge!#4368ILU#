  /**
   * Accessor for the width of the column
   *
   * @return the width
   */
  public int getWidth()
  {
    return width;
  }

  /**
   * Accessor for the hidden flag. Used when copying sheets
   *
   * @return TRUE if the columns are hidden, FALSE otherwise
   */
  public boolean getHidden()
  {
    return hidden;
  }
}


